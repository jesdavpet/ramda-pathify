{
  "name": "ramda-pathify",
  "version": "0.0.4",
  "description": "Simplifies creating path arrays for functions in the Ramda.js library.",
  "keywords": [
    "ramda",
    "path",
    "tag"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesdavpet/ramda-pathify-tag.git"
  },
  "homepage": "https://github.com/jesdavpet/ramda-pathify-tag#readme",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      [
        "latest",
        {}
      ],
      [
        "minify",
        {
          "mangle": {
            "topLevel": true
          }
        }
      ]
    ],
    "comments": false
  },
  "author": "Jesse David Peterson <@jesdavpet> (http://www.jes.dav.pet)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesdavpet/ramda-pathify-tag/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "build": "rimraf -rf ./lib && mkdir ./lib && babel src -d lib",
    "prepublish": "npm run build",
    "test": "npm run test:coverage",
    "test:once": "mocha **/**.test.js",
    "test:watch": "mocha -w **/**.test.js",
    "test:coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha **/**.test.js"
  }
}
